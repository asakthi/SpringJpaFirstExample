<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jpa.Student" table="StudentTable">
		<id name="sno" column="sno"></id>
		<property name="sname" column="sname"></property>
		<property name="semail" column="semail"></property>
		<property name="smobile" column="smobile"></property>

	</class>
</hibernate-mapping>